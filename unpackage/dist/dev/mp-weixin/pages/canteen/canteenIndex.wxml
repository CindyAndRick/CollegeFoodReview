<view class="canteenList"><view class="header"><block wx:if="{{canteenInfo.pictureList.length!=0}}"><view><swiper class="swiper" indicator-dots="true" autoplay="true" interval="3000" circular="true" indicator-active-color="white"><block wx:for="{{canteenInfo.pictureList}}" wx:for-item="item" wx:for-index="index"><swiper-item><image class="headImage" mode="aspectFill" src="{{item}}" data-event-opts="{{[['tap',[['progressImgClick',['$0',index],['canteenInfo.pictureList']]]]]}}" bindtap="__e"></image></swiper-item></block></swiper></view></block><block wx:else><image class="headImage" mode="aspectFill" src="../../static/null.jpg" data-event-opts="{{[['tap',[['progressImgClick',['$0','$1'],['canteenInfo.pictureList','index']]]]]}}" bindtap="__e"></image></block></view><image class="avatar" src="{{canteenInfo.avatarUrl}}"></image><view class="canteenInfo"><uni-section style="font-size:40rpx;" vue-id="1ea918c0-1" title="食堂信息" type="line" bind:__l="__l" vue-slots="{{['default']}}"><uni-group vue-id="{{('1ea918c0-2')+','+('1ea918c0-1')}}" mode="card" bind:__l="__l" vue-slots="{{['default']}}"><text class="text">食堂名称：</text><view class="info">{{''+canteenInfo.nickName+''}}</view><text class="text">地址：</text><view class="info">{{''+canteenInfo.address+''}}</view><text class="text">联系方式：</text><view class="info">{{''+canteenInfo.tel+''}}</view><text class="text">简介：</text><view class="info">{{''+canteenInfo.intro+''}}</view></uni-group></uni-section></view><view class="myButtonArea"><button data-event-opts="{{[['tap',[['editCanteen',['edit-canteen-info']]]]]}}" class="myButton" bindtap="__e">点击修改信息</button><button data-event-opts="{{[['tap',[['editDishes',['edit-dishes']]]]]}}" class="myButton" bindtap="__e">点击修改菜品</button></view></view>